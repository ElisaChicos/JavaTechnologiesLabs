<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                                  http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
              version="2.3">
    <navigation-rule>
        <from-view-id>/common/login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>adminDashboard</from-outcome>
            <to-view-id>/admin/adminDashboard.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>teacherDashboard</from-outcome>
            <to-view-id>/teacher/teacherDashboard.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>studentDashboard</from-outcome>
            <to-view-id>/student/studentDashboard.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/adminDashboard.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>evaluationConfig</from-outcome>
            <to-view-id>/admin/evaluationConfig.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
